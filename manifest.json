{
  "name": "Dark Theme",
  "version": "1.0.2",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://www.facebook.com/*"],
    "css": ["dark-style.css"]
  }],
  "browser_action": {
    "default_title": "Dark Theme"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
